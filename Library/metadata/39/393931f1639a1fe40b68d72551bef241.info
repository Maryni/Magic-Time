  <!-- EN: soaking wet -->
  <SoakingWet.stages.soaking_wet.label>xop</SoakingWet.stages.soaking_wet.label>
  <!-- EN: I'm soaking wet. -->
  <SoakingWet.stages.soaking_wet.description>estic xop</SoakingWet.stages.soaking_wet.description>
  
  <!-- EN: was imprisoned -->
